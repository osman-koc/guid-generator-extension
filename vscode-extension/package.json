{
  "name": "guidgenerator",
  "displayName": "GuidGenerator (uuid)",
  "description": "Generate and insert GUIDs effortlessly into your code using a simple shortcut.",
  "license": "SEE LICENSE IN LICENSE",
  "version": "0.0.3",
  "publisher": "kocdev",
  "author": {
    "name": "Osman Koc",
    "email": "info@osmkoc.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/osman-koc/guid-generator-extension"
  },
  "bugs": {
    "url": "https://github.com/osman-koc/guid-generator-extension/issues"
  },
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "GUID",
    "UUID",
    "Generate"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "extension.generateGuid",
        "title": "Generate GUID"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "extension.generateGuid",
          "group": "guidgenerator"
        }
      ]
    }
  },
  "keybindings": [
    {
      "command": "extension.generateGuid",
      "key": "ctrl+alt+g",
      "mac": "cmd+option+g",
      "when": "editorTextFocus"
    }
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  }
}
